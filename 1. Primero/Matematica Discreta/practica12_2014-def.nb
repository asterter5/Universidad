Cell["\<\
En la segona part empalmem els cicles de manera convenient per obtenir un \
cicle euleri\[AGrave]. Es comen\[CCedilla]a per un cicle qualsevol i es van \
empalmant la resta de cicles, un cicle cada vegada, de manera que el resultat \
\[EAcute]s un circuit sense arestes repetides.   \
\>", "Text",
 CellChangeTimes->{{3.6095602177376146`*^9, 3.609560258516086*^9}, {
  3.609586602357332*^9, 3.609586706404873*^9}, {3.6096512347668467`*^9, 
  3.6096512398212557`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"euleria", "[", "cycles_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cyc", ",", "euler", ",", "c", ",", "v"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"euler", ",", "cyc"}], "}"}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"First", "[", "cycles", "]"}], ",", 
        RowBox[{"Rest", "[", "cycles", "]"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", "  ", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "cyc", "]"}], ">", "0"}], ",", 
       "\[IndentingNewLine]", "                         ", 
       RowBox[{
        RowBox[{"c", "=", 
         RowBox[{"First", "[", 
          RowBox[{"Select", "[", 
           RowBox[{"cyc", ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Intersection", "[", 
                RowBox[{"euler", ",", "#"}], "]"}], "=!=", 
               RowBox[{"{", "}"}]}], ")"}], "&"}]}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "                         ", 
        RowBox[{"v", "=", 
         RowBox[{"First", "[", 
          RowBox[{"Intersection", "[", 
           RowBox[{"euler", ",", "c"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "                         ", 
        RowBox[{"euler", "=", 
         RowBox[{"Join", "[", " ", 
          RowBox[{
           RowBox[{"RotateLeft", "[", 
            RowBox[{"c", ",", 
             RowBox[{
              RowBox[{"Position", "[", 
               RowBox[{"c", ",", "v"}], "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "]"}], ",", 
           RowBox[{"RotateLeft", "[", 
            RowBox[{"euler", ",", 
             RowBox[{
              RowBox[{"Position", "[", 
               RowBox[{"euler", ",", "v"}], "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "]"}]}], "]"}]}], ";",
         "\[IndentingNewLine]", "                   ", 
        RowBox[{"cyc", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{"cyc", ",", 
           RowBox[{"{", "c", "}"}]}], "]"}]}]}]}], "]"}], ";", 
