Cell[BoxData[
 RowBox[{
  RowBox[{"cycEu", "[", "G_", "]"}], ":=", "\n", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", " ", 
     RowBox[{"Z", ",", 
      RowBox[{"R", "=", 
       RowBox[{"{", "}"}]}], ",", "G1"}], "}"}], ",", "\n", 
    RowBox[{
     RowBox[{"G1", "=", "G"}], ";", "\n", 
     RowBox[{"Z", "=", 
      RowBox[{"FindCycle", "[", "G1", "]"}]}], ";", "\n", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"Z", "\[NotEqual]", " ", 
        RowBox[{"{", "}"}]}], ",", "\n", 
       RowBox[{
        RowBox[{"G1", "=", 
         RowBox[{"DeleteCycle", "[", 
          RowBox[{"G1", ",", "Z"}], "]"}]}], ";", "\n", 
        RowBox[{"R", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"R", ",", "Z"}], "]"}]}], ";", "\n", 
        RowBox[{"Z", "=", 
         RowBox[{"FindCycle", "[", "G1", "]"}]}], ";"}]}], "\n", "]"}], ";", 
     "\n", "R"}]}], "\n", "]"}]}]], "Input",
