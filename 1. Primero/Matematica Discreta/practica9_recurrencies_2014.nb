Cell[TextData[{
 "\n\nPer a determinar les solucions de la recurr\[EGrave]ncia:\n             \
                        \n                                        ",
 Cell[BoxData[
  FormBox[
   FrameBox[
    RowBox[{" ", 
     RowBox[{
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"n", "+", "2"}]], "+", " ", 
       RowBox[{
        SubscriptBox["c", "1"], "\[CenterDot]", 
        SubscriptBox["a", 
         RowBox[{"n", "+", "1"}]]}], "+", " ", 
       RowBox[{
        SubscriptBox["c", "0"], "\[CenterDot]", " ", 
        SubscriptBox["a", "n"]}]}], " ", "=", "0"}]}]], TraditionalForm]]],
 "\n                                        \n  considerem el",
 StyleBox[" polinomi caracter\[IAcute]stic associat",
  FontColor->RGBColor[0, 0, 1]],
 ":\n  \n                                            ",
 Cell[BoxData[
  FormBox[
   FrameBox[
    RowBox[{" ", 
     RowBox[{
      RowBox[{"p", "(", "x", ")"}], "=", 
      RowBox[{
       SuperscriptBox["x", "2"], "+", 
       RowBox[{
        SubscriptBox["c", "1"], "\[CenterDot]", "x"}], "+", 
       SubscriptBox["c", "0"]}]}]}]], TraditionalForm]]],
 "\n \n  i en calculem les arrels, llavors tenim els cassos \
seg\[UDoubleDot]ents:\n                    \n                    - Dues \
arrels reals diferents: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", ",", 
    RowBox[{"\[Beta]", "."}]}], TraditionalForm]]],
 " La soluci\[OAcute] \[EAcute]s:\n                    \n                     \
                           ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "1"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CenterDot]", 
    SuperscriptBox["\[Alpha]", "n"]}], TraditionalForm]]],
 " + ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "2"], TraditionalForm]]],
 "\[CenterDot]",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Beta]", "n"], TraditionalForm]]],
 "\n                    \n                    - Una arrel real doble: \
\[Alpha]. La soluci\[OAcute] \[EAcute]s:\n                    \n              \
                                 ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "1"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CenterDot]", 
    SuperscriptBox["\[Alpha]", "n"]}], TraditionalForm]]],
 " + ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "2"], TraditionalForm]]],
 "\[CenterDot]n\[CenterDot]",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Alpha]", "n"], TraditionalForm]]],
 "\n                    \n                    - Dues arrels complexes \
conjugades: \[Alpha]\[PlusMinus]\[Beta]i. La soluci\[OAcute] \[EAcute]s:\n    \
                \n                                               ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "1"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CenterDot]", 
    SuperscriptBox["r", "n"]}], TraditionalForm]]],
 "\[CenterDot] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Cos", "[", 
    RowBox[{"\[Theta]", "\[CenterDot]", "n"}], "]"}], TraditionalForm]]],
 " +",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    SubscriptBox["K", "2"]}], TraditionalForm]]],
 "\[CenterDot]",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "n"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CenterDot]", 
    RowBox[{"Sin", "[", 
     RowBox[{"\[Theta]", "\[CenterDot]", "n"}], "]"}]}], TraditionalForm]]],
 "\n                    \nEls coeficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "1"], TraditionalForm]]],
 ",",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    SubscriptBox["K", "2"]}], TraditionalForm]]],
 "  de la soluci\[OAcute] es determinen a partir de les condicions inicials.  \
                  \n"
}], "Text",
